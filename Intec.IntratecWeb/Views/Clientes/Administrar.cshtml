
@{
    ViewBag.Title = "Administrar";
}
<script>
    var cliente = null;
    $(document).ready(() => {
        _peticionAjax_Promise(URL_SERVICE + URI_SERVICE.ObtenerCliente, VerbosREST.POST, JSON.stringify({ sessionToken: UsuarioSesion.tokenSesion, idCliente: window.location.href.split('=')[1] }))
            .then(response => {
                cliente = response.data;
                MostrarCliente();
            });
    });

    function MostrarCliente() {
        if (cliente != null) {
            console.log(cliente);
        }
    }
</script>
<h3 class="text-center" id="title">ADMINISTRAR CLIENTE</h3>
<div class="card">
    <h3>Info General</h3>
</div>
<div class="card">
    <h3>Propiedades</h3>
</div>
<div class="card">
    <h3>Visitas</h3>
</div>